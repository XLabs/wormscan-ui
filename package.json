		"deploy": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
	},
	"devDependencies": {
		"@parcel/transformer-sass": "2.8.3",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"husky": ">=6",
		"lint-staged": ">=10",
		"parcel": "^2.8.3",
		"prettier": "^2.8.3",
		"process": "^0.11.10",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@radix-ui/react-icons": "^1.1.1",
		"@radix-ui/react-toggle-group": "^1.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"alias": {
		"src": "./src"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,scss,css,md}": "prettier --write"
